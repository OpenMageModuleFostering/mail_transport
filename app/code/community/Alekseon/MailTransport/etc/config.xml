<?xml version="1.0"?><!--/** * @author    Marcin Frymark * @email     contact@alekseon.com * @company   Alekseon * @website   www.alekseon.com */--><config>    <modules>        <Alekseon_MailTransport>            <version>0.1.0</version>        </Alekseon_MailTransport>    </modules>    <global>        <models>            <alekseon_mailTransport>                <class>Alekseon_MailTransport_Model</class>            </alekseon_mailTransport>            <core>                <rewrite>                    <email_template>Alekseon_MailTransport_Model_Email_Template</email_template>                </rewrite>            </core>        </models>        <blocks>            <alekseon_mailTransport>                <class>Alekseon_MailTransport_Block</class>            </alekseon_mailTransport>        </blocks>        <helpers>            <alekseon_mailTransport>                <class>Alekseon_MailTransport_Helper</class>            </alekseon_mailTransport>        </helpers>    </global>    <adminhtml>        <events>            <controller_action_predispatch>                <observers>                    <alekseon_adminNotification>                        <class>alekseon_mailTransport/alekseonAdminNotification_observer</class>                        <method>preDispatch</method>                    </alekseon_adminNotification>                </observers>            </controller_action_predispatch>        </events>    </adminhtml>    <default>        <alekseon_adminNotification>            <general>                <enabled>1</enabled>                <frequency>12</frequency>            </general>        </alekseon_adminNotification>        <alekseon_mailTransport>            <general>                <smtp_password backend_model="adminhtml/system_config_backend_encrypted"/>            </general>        </alekseon_mailTransport>    </default></config>